// see application for notices related styling

@import 'palette';

.warning {
  background-color: $color-primary-4;
}

.passed {
  background-color: $color-complement-4;
}

.subtle {
  color: $color-secondary-2-2;
}

.disabled {
  color: $color-secondary-2-2;
}

.mx-spinner {
  position: absolute;
  background-image: image-url('spinner.gif'); 
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-color: #aeaeae;
  opacity: 0.9;
}

.instruction {
    color: $color-secondary-2-2;


}
